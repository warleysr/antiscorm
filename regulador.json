{
    "nome": "regulador de tensao",
    "questoes": 10,
    "textos": [
        "Admitir que a regulação do circuito abaixo está no limite de Vimin. Dados:",
        "calcule a tensão de ondulação no capacitor."
    ],
    "formulas": {
        "Vpsec": "${2}",
        "ViMax": "${Vpsec} - ${4}",
        "Treg": "${1}",
        "ViMin": "${Treg} + 3",
        "Vond": "${ViMax} - ${ViMin}",
        "Imax0": "${Treg} / ${0}",
        "Imax1": "${0} / ${Treg}",
        "C": "${Imax} / (${3} * ${Vond})"
    },
    "condicionais": {
        "Imax": {
            "RL": "Imax0",
            "Potência em RL": "Imax1"
        }
    },
    "desejado": {
        "capacitor.": "Vond",
        ").": "C"
    }
}