{
    "nome": "regulador de tensao",
    "questoes": 10,
    "regex": {
        "PotRL": "Potência em RL = (\\d{1,}(?:\\.\\d{1,})?) W",
        "RL": "RL = (\\d{1,}(?:\\.\\d{1,})?) ohms",
        "Vpsec": "Vi = (\\d{1,}) (Vp|Vpp|Vrms)",
        "F": "/ (\\d{1,}) Hz",
        "Vd": "Vd = (\\d{1,}.\\d{1,}) V",
        "C": "C = (\\d{1,}.\\d{1,}) (uF)",
        "Treg": "LM(?:78|79)(\\d{2,2})"
    },
    "formulas": {
        "ViMax": "${Vpsec} - ${Vd}",
        "ViMin": "${Treg} + 3",
        "Vond": "${ViMax} - ${ViMin}",
        "VdcC": "${ViMax} - (${Vond} / 2)",
        "Imax0": "${Treg} / ${RL}",
        "Imax1": "${PotRL} / ${Treg}",
        "C": "${Imax->@} / (${F} * ${Vond})"
    },
    "objetivo": {
        "ondulação no capacitor": "Vond",
        "média no capacitor": "VdcC",
        "sem uF)": "C->*"
    }
} 